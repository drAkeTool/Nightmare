{
    "name": "dmzx/totalavtiveext",
    "type": "phpbb-extension",
    "description": "This extension shows total active extensions on index.",
    "homepage": "http://www.dmzx-web.net",
    "version": "1.0.0",
    "time": "2015-09-05",
    "keywords": [
        "phpbb",
        "extension",
        "Total Active Extensions"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "dmzx",
            "homepage": "http://www.dmzx-web.net",
            "email": "info@dmzx-web.net",
            "role": "Extension Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "extra": {
        "display-name": "Total Active Extensions",
        "soft-require": {
            "phpbb/phpbb": "3.1.*"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/total_active_extensions",
            "filename": "version_check"
        }
    }
}